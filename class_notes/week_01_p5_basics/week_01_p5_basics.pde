// the size function creates a window 
// that is width, height pixels big

//scaling of window, measured in pixels
size(500, 500);

// the fill of the background
background(250,0,0);

// colors the object the same color of my sweater
fill(250,250,225);

// the color of the surrounding pixels of an object
stroke(255,255,255);

//width of the stroke 
strokeWeight(5);

// X1 is the x coordinate, Y1 is the y coordniate, X2 is width, and Y2 fourth is height
rect(30, 40, 60, 60);

rect(60, 60, 60, 60);

rect(90, 90, 60, 60);

//draw a circle...
ellipse(222,222,80,80);

//draw a line x1, y1, x2, y2
line(0, height, width, 0);